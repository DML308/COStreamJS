import Matrix;
Matrix A = Matrix.random(100,100);
Matrix B = Matrix.random(100,100);
composite Main(){
    stream<Matrix x>S1;
    S1=First(){
        work{
            S1[0].x = A*B;
        }
        window{
            S1 tumbling(1);
        }
    };
    Sink(S1){
        work{}
        window{
            S1 sliding(1,1);
        }
    }
}