  stream<Matrix x> In,In0,In1, Out, hidden, Y,hidden_outputs,hidden_outputs0,hidden_outputs1,hidden_outputs2 ;
  stream<Matrix x>  final_outputs,output_errors,output_errors0,output_errors1,hidden_errors, final_outputs0,final_outputs1;
  
