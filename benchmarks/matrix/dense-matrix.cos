composite loss(input stream<Matrix x>In,stream<Matrix x>Y, output stream<Matrix x>Out){
  Out=loss(In,Y){
    work{
      Out[0].x = Y[0].x - In[0].x ;    
      //println(In[0].x);
    }
    window{
      In sliding(1,1);
      Y sliding(1,1);
      Out tumbling(1,1);
    }
  }
  ;
}
composite dDense_2(input stream<Matrix x>In0,stream<Matrix x>In1,stream<Matrix x>In2, output stream<Matrix x>Out){
  Out=dDense2(In0,In1,In2){
    work{
      /** Matrix hidden_errors = outputs_errors * _weight_3.transpose();

        Matrix m = (hidden_outputs.transpose() * 0.1 * outputs_errors.cwiseProduct(final_outputs).cwiseProduct((Matrix::Constant(1,10,1)-final_outputs)));
        _weight_3 += m; */
      Out[0].x = In0[0].x * _weight_3.transpose();
      
      double lr=0.1;
      Matrix hidden_errors = Out[0].x;
      Matrix outputs_errors = In0[0].x;
      Matrix final_outputs = In1[0].x;
      Matrix hidden_outputs = In2[0].x;
      Matrix m = (hidden_outputs.transpose() * 0.1 * outputs_errors.cwiseProduct(final_outputs).cwiseProduct((Matrix.Constant(1,10,1)-final_outputs)));
      _weight_3 += m;
      
    }
    window{
      In0 sliding(1,1);
      In1 sliding(1,1);
      In2 sliding(1,1);
      Out tumbling(1,1);
    }
  }
  ;
}
