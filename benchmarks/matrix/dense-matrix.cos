composite dense_1(input stream<Matrix x>In, output stream<Matrix x>Out){
  Out=dense_1(In){
    work{
      /** Matrix hidden_inputs = In * _weight_1;
          Matrix hidden_outputs = 1/ (1+( - hidden_inputs.array() ).exp()); */
